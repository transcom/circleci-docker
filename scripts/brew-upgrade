#!/bin/bash

#
# A script to upgrade all brew formula related to docker files
#

set -exu -o pipefail

brew upgrade \
    awscli \
    chamber \
    circleci \
    go \
    go-bindata \
    node@10 \
    pre-commit \
    shellcheck \
    terraform-docs \
    liamg/tfsec/tfsec \
    tfenv \
    yarn

tfenv install latest
tfenv use latest
